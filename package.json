{
    "name": "@agentscript-ai/demo-dunder-mifflin",
    "repository": {
        "type": "git",
        "url": "https://github.com/AgentScript-AI/demo-dunder-mifflin.git"
    },
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "private": true,
    "scripts": {
        "upgrade:agentscript": "yarn up agentscript-ai \"@agentscript-ai/*\"",
        "upgrade:nzyme": "yarn up \"@nzyme/*\"",
        "upgrade:interactive": "yarn upgrade-interactive",
        "db:start": "yarn supabase start",
        "db:push": "yarn --cwd packages/database db:push",
        "db:seed": "yarn --cwd packages/database db:seed",
        "monorepo": "nzyme monorepo",
        "build": "nzyme monorepo && tsc -b ./tsconfig.dev.json",
        "setup": "nzyme monorepo && yarn build && yarn db:start && yarn db:push"
    },
    "devDependencies": {
        "@agentscript-ai/tsconfig": "^0.4.0",
        "@nzyme/cli": "^0.5.0",
        "prettier": "^3.4.2",
        "supabase": "2.6.8",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    },
    "workspaces": [
        "packages/*"
    ],
    "packageManager": "yarn@4.6.0"
}
